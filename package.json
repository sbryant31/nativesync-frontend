{
  "name": "nativesync-frontend",
  "version": "1.0.0",
  "description": "",
  "engineStrict": true,
  "engines": {
    "npm": "^3.4.1"
  },
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "postinstall": "npm run build",
    "build": "mkdir -p dist/node_modules && browserify app/index.js -d -p [minifyify --map app.js.map --output dist/app.js.map] -o dist/app.js",
    "postbuild": "cp -r img dist/ && cp index.html dist/ && cp -r node_modules/@blueprintjs dist/node_modules/",
    "watch": "mkdir -p dist/node_modules && watchify --poll=1000 app/index.js -d -p [minifyify --map app.js.map --output dist/app.js.map] -o dist/app.js -v",
    "dev": "budo app/index.js -s app.js --pushstate",
    "start": ""
  },
  "browserify": {
    "transform": [
      "browserify-css",
      "envify",
      [
        "babelify",
        {
          "presets": [
            "es2015",
            "react"
          ]
        }
      ]
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@bitbucket.org/nativesyncdev/nativesync-frontend.git"
  },
  "author": "",
  "license": "ISC",
  "homepage": "https://bitbucket.org/nativesyncdev/nativesync-frontend#readme",
  "dependencies": {
    "@blueprintjs/core": "^1.3.1",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-react": "^6.16.0",
    "babelify": "^7.3.0",
    "bluebird": "^3.4.6",
    "blueprintjs": "0.0.8",
    "bootstrap": "^3.3.7",
    "browserify": "^13.1.1",
    "browserify-css": "^0.9.2",
    "budo": "^9.3.0",
    "envify": "^4.0.0",
    "flexboxgrid": "^6.3.1",
    "lodash": "^4.17.2",
    "md5": "^2.2.1",
    "minifyify": "^7.3.4",
    "node-static": "^0.7.9",
    "normalize.css": "^5.0.0",
    "react": "^15.4.1",
    "react-addons-css-transition-group": "^15.4.1",
    "react-codemirror": "^0.3.0",
    "react-dom": "^15.4.1",
    "react-json": "^0.2.1",
    "react-json-tree": "^0.10.0",
    "react-markdown-editor": "^0.4.0",
    "react-numeric-input": "^2.0.6",
    "react-router": "^2.8.1",
    "react-select": "^1.0.0-rc.2",
    "react-tools": "^0.13.3",
    "request": "^2.75.0",
    "request-promise": "^4.1.1",
    "statesync": "^0.1.2",
    "store": "^1.3.20",
    "underscore": "^1.8.3",
    "url-join": "^1.1.0",
    "watchify": "^3.8.0"
  }
}
