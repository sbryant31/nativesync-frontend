{
  "name": "nativesync-frontend",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "mkdir -p public && browserify app/index.js -d -p [minifyify --map app.js.map --output public/app.js.map] -o public/app.js",
    "watch": "mkdir -p public && watchify app/index.js -d -p [minifyify --map app.js.map --output public/app.js.map] -o public/app.js -v",
    "dev": "budo app/index.js -s public/app.js --pushstate",
    "start": "node index.js"
  },
  "browserify": {
    "transform": [
      "browserify-css",
      [
        "babelify",
        {
          "presets": [
            "es2015",
            "react"
          ]
        }
      ]
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@bitbucket.org/nativesyncdev/nativesync-frontend.git"
  },
  "author": "",
  "license": "ISC",
  "homepage": "https://bitbucket.org/nativesyncdev/nativesync-frontend#readme",
  "devDependencies": {
    "@blueprintjs/core": "^1.1.0",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-react": "^6.16.0",
    "babelify": "^7.3.0",
    "bluebird": "^3.4.6",
    "bootstrap": "^3.3.7",
    "browserify": "^13.1.1",
    "browserify-css": "^0.9.2",
    "budo": "^9.2.2",
    "flexboxgrid": "^6.3.1",
    "lodash": "^4.17.2",
    "minifyify": "^7.3.4",
    "react": "^15.4.1",
    "react-addons-css-transition-group": "^15.4.1",
    "react-dom": "^15.4.0",
    "react-router": "^2.8.1",
    "react-tools": "^0.13.3",
    "request": "^2.75.0",
    "request-promise": "^4.1.1",
    "statesync": "^0.1.2",
    "store": "^1.3.20",
    "watchify": "^3.7.0"
  },
  "dependencies": {
    "node-static": "^0.7.9"
  }
}
